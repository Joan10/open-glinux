# Configuration file for ddclient generated by debconf
#
# /etc/ddclient.conf

protocol=dyndns2
use=if, if=br-dmz
use=web, web=http://checkip.dyndns.org/, web-skip='IP Address'
server={{ ddclient_server }}
login={{ ddclient_username }}
password={{ ddclient_password }}
syslog=yes
{{ ddclient_name }}
